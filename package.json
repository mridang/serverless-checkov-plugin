{
  "name": "serverless-checkov-plugin",
  "version": "0.0.1",
  "scripts": {
    "prepack": "tsc",
    "build": "tsc",
    "test": "jest --verbose --config=jest.config.js --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:debug": "jest --verbose --config=jest.config.js --runInBand --detectOpenHandles",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,md,html}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,md,html}\"",
    "lint": "npx eslint --ext .jsx,.js,.tsx,.ts .",
    "lint:fix": "npx eslint --ext .jsx,.js,.tsx,.ts . --fix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "serverless",
    "serverless framework",
    "serverless plugin",
    "checkov"
  ],
  "author": {
    "name": "Mridang Agarwalla",
    "email": "mridang.agarwalla@gmail.com",
    "url": "https://github.com/mridang"
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "dependencies": {
  },
  "description": "A plugin for the serverless framework to inspect the setup via Checkov",
  "private": false,
  "peerDependencies": {
    "serverless": "^3.38.0"
  },
  "engines": {
    "node": "20.x"
  },
  "repository": "https://github.com/mridang/serverless-checkov-plugin",
  "homepage": "https://github.com/mridang/serverless-checkov-plugin",
  "bugs": "https://github.com/mridang/serverless-checkov-plugin/issues",
  "overrides": {
    "eslint-config-standard-with-typescript": {
      "@typescript-eslint/eslint-plugin": "^7.0.0"
    },
    "ip": "2.0.1"
  },
  "devDependencies": {
    "@serverless/test": "^11.1.1",
    "@tsconfig/node20": "latest",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.3.2",
    "@types/serverless": "^3.12.22",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.0.2",
    "@typescript-eslint/typescript-estree": "^7.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "npm": "^10.4.0",
    "prettier": "^3.1.1",
    "serverless": "^3.38.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
